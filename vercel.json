{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1.js" }
  ],
  "redirects": [
    { "source": "/", "destination": "/assessment.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
